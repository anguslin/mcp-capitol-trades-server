{
  "name": "@anguslin/mcp-capitol-trades",
  "version": "1.0.1",
  "description": "MCP Server for extracting politician stock trades with prices from Capitol Trades. No API key required.",
  "type": "module",
  "main": "build/src/index.js",
  "bin": {
    "mcp-capitol-trades": "build/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "dev": "tsc --watch",
    "start": "node build/src/index.js",
    "verify": "node verify.js",
    "test:scraper": "tsx test/test-scraper.ts"
  },
  "keywords": [
    "mcp",
    "capitol-trades",
    "politician-trades",
    "congress-trades",
    "congressional-trading",
    "stock-trades",
    "model-context-protocol",
    "congress",
    "trading-disclosures",
    "politician-stocks",
    "insider-trading",
    "claude",
    "cursor"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anguslin/mcp-capitol-trades.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "playwright": "^1.56.1",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
